<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!--1.获取bean演示 创建user对象-->
    <bean id="user" class="com.atguigu.spring6.iocxml.User"/>
    <bean id="user1" class="com.atguigu.spring6.iocxml.User"/>

    <!--2.一个接口实现类获取过程-->
    <bean id="userDao" class="com.atguigu.spring6.iocxml.bean.UserDaoImpl"/>
    <bean id="personDao" class="com.atguigu.spring6.iocxml.bean.PersonDaoImpl"/>
</beans>